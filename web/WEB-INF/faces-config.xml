<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>../view/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{AlaskaControl.check}</from-action>
            <from-outcome>view</from-outcome>
            <to-view-id>../view/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{AlaskaControl.check}</from-action>
           <from-outcome>fail</from-outcome>
            <to-view-id>../view/index.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{RegisterControl.sucessReg}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>../view/success.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{RegisterControl.sucessReg}</from-action>
            <from-outcome>equal</from-outcome>
            <to-view-id>../view/equal.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{RegisterControl.sucessReg}</from-action>
           <from-outcome>exception</from-outcome>
            <to-view-id>../view/exception.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{AlaskaControl.check}</from-action>
            <from-outcome>LogException</from-outcome>
            <to-view-id>../view/LogException.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{Winter.viewWinterProduct}</from-action>
            <from-outcome>MenWinter</from-outcome>
            <to-view-id>../view/MenWinter.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{Summer.viewSummerProduct}</from-action>
            <from-outcome>MenSummer</from-outcome>
            <to-view-id>../view/MenSummer.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{Footwear.viewFootwearProduct}</from-action>
           <from-outcome>MenFootWear</from-outcome>
            <to-view-id>../view/MenFootWear.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{WomSummer.viewSummerProduct}</from-action>
            <from-outcome>WomenSummer</from-outcome>
            <to-view-id>../view/WomenSummer.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{WomWinter.viewWinterProduct}</from-action>
            <from-outcome>WomenWinter</from-outcome>
            <to-view-id>../view/WomenWinter.xhtml</to-view-id>
        </navigation-case>


        <navigation-case>
            <from-action>#{WomFootwear.viewFootwearProduct}</from-action>
            <from-outcome>WomenFootWear</from-outcome>
            <to-view-id>../view/WomenFootWear.xhtml</to-view-id>
        </navigation-case>


        <navigation-case>
            <from-action>#{chSummmer.viewSummerProduct}</from-action>
            <from-outcome>ChildSummer</from-outcome>
            <to-view-id>../view/ChildSummer.xhtml</to-view-id>
        </navigation-case>


        <navigation-case>
            <from-action>#{chWinter.viewWinterProduct}</from-action>
            <from-outcome>ChildWinter</from-outcome>
            <to-view-id>../view/ChildWinter.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{chFootWear.viewFootwearProduct}</from-action>
            <from-outcome>ChildFootwear</from-outcome>
            <to-view-id>../view/ChildFootwear.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{Developer.viewINFO}</from-action>
            <from-outcome>DevelopeInformation</from-outcome>
            <to-view-id>../view/DevelopeInformation.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{Tailor.viewTailorInfo}</from-action>
            <from-outcome>TailorInfo</from-outcome>
            <to-view-id>../view/TailorInfo.xhtml</to-view-id>
        </navigation-case>


    </navigation-rule>


  
</faces-config>