<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>../view/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{AlaskaControl.check}</from-action>
            <from-outcome>view</from-outcome>
            <to-view-id>../view/view.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{AlaskaControl.check}</from-action>
           <from-outcome>fail</from-outcome>
            <to-view-id>../view/index.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{RegisterControl.sucessReg}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>../view/success.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{RegisterControl.sucessReg}</from-action>
            <from-outcome>equal</from-outcome>
            <to-view-id>../view/equal.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{RegisterControl.sucessReg}</from-action>
           <from-outcome>exception</from-outcome>
            <to-view-id>../view/exception.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{AlaskaControl.check}</from-action>
            <from-outcome>LogException</from-outcome>
            <to-view-id>../view/LogException.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-action>#{Select.viewSelection}</from-action>
            <from-outcome>collection</from-outcome>
            <to-view-id>../view/collection.xhtml</to-view-id>
        </navigation-case>

<navigation-case>
    <from-action>#{Select.viewselection}</from-action>
    <from-outcome>Men</from-outcome>
    <to-view-id>../view/Men.xhtml</to-view-id>
</navigation-case>



    </navigation-rule>


  
</faces-config>